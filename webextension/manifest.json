{
	"manifest_version": 2,
	"name": "Nepi Jano!",
	"version": "2.0",
	"description": "Read paid content on www.sme.sk",
	"permissions": ["cookies", "webRequest", "*://*.sme.sk/c/*"],
	"content_scripts": [
		{
			"js": ["article.js"],
			"matches": ["*://*.sme.sk/c/*"]
		},
		{
			"js": ["video.js"],
			"matches": ["http://tv.sme.sk/v*"]
		}
	],
	"homepage_url": "https://github.com/viliampucik/nepi-jano",
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	}
}